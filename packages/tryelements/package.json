  "author": "Railly Hugo <the.crafter.station@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crafter-station/elements",
    "directory": "packages/tryelements"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
