{
  "name": "@tryelements/cli",
  "version": "0.1.0",
  "description": "Install SVG logos from tryelements.dev",
  "type": "module",
  "bin": {
    "tryelements": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
