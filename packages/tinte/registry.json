{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "elements-tinte",
  "homepage": "https://www.tryelements.dev",
  "items": [
    {
      "name": "theme-editor",
      "type": "registry:block",
      "title": "Tinte Theme Editor",
      "description": "Live theme editor with AI-powered theme generation, Browse tab for tinte.dev themes, and manual color editing. Complete shadcn/ui theme customization tool.",
      "registryDependencies": [
        "button",
        "dialog",
        "tabs",
        "textarea",
        "accordion",
        "scroll-area",
        "input"
      ],
      "dependencies": [
        "@ai-sdk/openai",
        "@radix-ui/react-accordion",
        "@radix-ui/react-dialog",
        "@radix-ui/react-scroll-area",
        "@radix-ui/react-tabs",
        "@uiw/color-convert",
        "@uiw/react-color",
        "ai",
        "culori",
        "lucide-react",
        "nanoid",
        "zod"
      ],
      "files": [
        {
          "path": "registry/default/theme-editor/components/theme-editor.tsx",
          "type": "registry:component",
          "target": "@/components/theme-editor.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/color-input.tsx",
          "type": "registry:component",
          "target": "@/components/color-input.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/logo.tsx",
          "type": "registry:component",
          "target": "@/components/tinte-logo.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/chat/chat-input.tsx",
          "type": "registry:component",
          "target": "@/components/chat/chat-input.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/chat/message.tsx",
          "type": "registry:component",
          "target": "@/components/chat/message.tsx"
        },
        {
          "path": "registry/default/theme-editor/lib/tinte-to-shadcn.ts",
          "type": "registry:lib",
          "target": "@/lib/tinte-to-shadcn.ts"
        },
        {
          "path": "registry/default/theme-editor/lib/utils.ts",
          "type": "registry:lib",
          "target": "@/lib/utils.ts"
        },
        {
          "path": "registry/default/theme-editor/components/ui/accordion.tsx",
          "type": "registry:ui",
          "target": "@/components/ui/accordion.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/ui/button.tsx",
          "type": "registry:ui",
          "target": "@/components/ui/button.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/ui/dialog.tsx",
          "type": "registry:ui",
          "target": "@/components/ui/dialog.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/ui/input.tsx",
          "type": "registry:ui",
          "target": "@/components/ui/input.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/ui/scroll-area.tsx",
          "type": "registry:ui",
          "target": "@/components/ui/scroll-area.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/ui/tabs.tsx",
          "type": "registry:ui",
          "target": "@/components/ui/tabs.tsx"
        },
        {
          "path": "registry/default/theme-editor/components/ui/textarea.tsx",
          "type": "registry:ui",
          "target": "@/components/ui/textarea.tsx"
        }
      ],
      "envVars": {
        "OPENAI_API_KEY": "sk-..."
      },
      "docs": "https://www.tryelements.dev/docs/tinte"
    }
  ]
}
