{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "elements-tinte",
  "homepage": "https://www.tryelements.dev",
  "items": [
    {
      "name": "tinte-editor",
      "type": "registry:block",
      "title": "Tinte Theme Editor",
      "description": "Live theme editor with AI-powered theme generation, Browse tab for tinte.dev themes, and manual color editing. Complete shadcn/ui theme customization tool.",
      "registryDependencies": [
        "button",
        "input",
        "label",
        "popover",
        "dialog",
        "tabs",
        "textarea",
        "accordion",
        "@elements/tinte-logo"
      ],
      "dependencies": [
        "@ai-sdk/openai",
        "@radix-ui/react-accordion",
        "@radix-ui/react-dialog",
        "@radix-ui/react-scroll-area",
        "@radix-ui/react-tabs",
        "@uiw/color-convert",
        "@uiw/react-color",
        "streamdown",
        "ai",
        "culori",
        "lucide-react",
        "server-only"
      ],
      "files": [
        {
          "path": "src/registry/tinte/tinte-editor/api/write-globals/route.ts",
          "type": "registry:page",
          "target": "app/api/elements/tinte/write-globals/route.ts"
        },
        {
          "path": "src/registry/tinte/tinte-editor/api/read-globals/route.ts",
          "type": "registry:page",
          "target": "app/api/elements/tinte/read-globals/route.ts"
        },
        {
          "path": "src/registry/tinte/tinte-editor/components/tinte-editor.tsx",
          "type": "registry:component",
          "target": "components/elements/tinte/tinte-editor.tsx"
        },
        {
          "path": "src/registry/tinte/tinte-editor/components/color-input.tsx",
          "type": "registry:component",
          "target": "components/elements/tinte/color-input.tsx"
        },
        {
          "path": "src/registry/tinte/tinte-editor/components/chat/chat-container.tsx",
          "type": "registry:component",
          "target": "components/elements/tinte/chat-container.tsx"
        },
        {
          "path": "src/registry/tinte/tinte-editor/components/chat/chat-input.tsx",
          "type": "registry:component",
          "target": "components/elements/tinte/chat-input.tsx"
        },
        {
          "path": "src/registry/tinte/tinte-editor/components/chat/chat-message.tsx",
          "type": "registry:component",
          "target": "components/elements/tinte/chat-message.tsx"
        },
        {
          "path": "src/registry/tinte/tinte-editor/lib/tinte-to-shadcn.ts",
          "type": "registry:lib"
        },
        {
          "path": "src/registry/tinte/tinte-editor/lib/get-css-path.ts",
          "type": "registry:lib"
        }
      ],
      "envVars": {
        "OPENAI_API_KEY": "sk-..."
      },
      "docs": "https://www.tryelements.dev/docs/tinte"
    }
  ]
}
