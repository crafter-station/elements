{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "tinte-editor",
  "type": "registry:block",
  "title": "Tinte Theme Editor",
  "description": "Live theme editor with AI-powered theme generation, Browse tab for tinte.dev themes, and manual color editing. Complete shadcn/ui theme customization tool. Works entirely client-side with DOM-based CSS variable management.",
  "registryDependencies": [
    "button",
    "input",
    "label",
    "popover",
    "dialog",
    "tabs",
    "textarea",
    "accordion",
    "@elements/tinte-logo"
  ],
  "dependencies": [
    "@ai-sdk/openai",
    "@radix-ui/react-accordion",
    "@radix-ui/react-dialog",
    "@radix-ui/react-scroll-area",
    "@radix-ui/react-tabs",
    "@uiw/color-convert",
    "@uiw/react-color",
    "streamdown",
    "ai",
    "culori",
    "lucide-react"
  ],
  "files": [
    {
      "path": "registry/default/blocks/tinte-editor/page.tsx",
      "type": "registry:page",
      "target": "app/elements/tinte/page.tsx"
    },
    {
      "path": "registry/default/blocks/tinte-editor/components/tinte-editor.tsx",
      "type": "registry:file",
      "target": "components/elements/tinte/tinte-editor.tsx"
    },
    {
      "path": "registry/default/blocks/tinte-editor/components/color-input.tsx",
      "type": "registry:component",
      "target": "components/elements/tinte/color-input.tsx"
    },
    {
      "path": "registry/default/blocks/tinte-editor/components/chat-container.tsx",
      "type": "registry:component",
      "target": "components/elements/tinte/chat-container.tsx"
    },
    {
      "path": "registry/default/blocks/tinte-editor/components/chat-input.tsx",
      "type": "registry:component",
      "target": "components/elements/tinte/chat-input.tsx"
    },
    {
      "path": "registry/default/blocks/tinte-editor/components/chat-message.tsx",
      "type": "registry:component",
      "target": "components/elements/tinte/chat-message.tsx"
    },
    {
      "path": "registry/default/blocks/tinte-editor/lib/tinte-to-shadcn.ts",
      "type": "registry:lib",
      "target": "lib/tinte-to-shadcn.ts"
    }
  ],
  "envVars": {
    "OPENAI_API_KEY": "sk-..."
  },
  "docs": "https://www.tryelements.dev/docs/tinte"
}
